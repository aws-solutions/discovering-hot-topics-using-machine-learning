{
    "PhysicalTableMap": {
        "274f2c2f-89bc-4da4-8ab7-860e969cde83": {
            "CustomSql": {
                "DataSourceArn": "{self.data_source.arn}",
                "Name": "image-moderation-label",
                "SqlQuery": "with s as (select distinct id_str, platform, text, translated_text, created_at from socialmediadb.sentiment where created_at > current_timestamp - interval '15' day), feed as (select distinct tw.db_created_at as created_at, tw.id_str, tw.platform, tw.account_name, tw.user.id_str as source, tw.user.name as name, tw.in_reply_to_screen_name from socialmediadb.twfeedstorage tw where tw.db_created_at > current_timestamp - interval '15' day union select distinct rss.created_at, rss.id_str, rss.platform, rss.account_name, rss.entities.urls[1].expanded_url as source, rss.metadata.topic as name, rss.metadata.website from socialmediadb.newsfeedstorage rss where rss.created_at > current_timestamp - interval '15' day AND cardinality(rss.entities.urls) > 0), img_m as (select distinct platform, id_str, image_url, label_name, confidence from socialmediadb.moderationlabels  where created_at > current_timestamp - interval '15' day) select distinct s.id_str, s.platform, s.text, s.translated_text, s.created_at, img_m.image_url, img_m.label_name, img_m.confidence, feed.source, feed.name from s join img_m on (img_m.id_str = s.id_str) join feed on (s.id_str = feed.id_str)",
                "Columns": [
                    {
                        "Name": "id_str",
                        "Type": "STRING"
                    },
                    {
                        "Name": "platform",
                        "Type": "STRING"
                    },
                    {
                        "Name": "text",
                        "Type": "STRING"
                    },
                    {
                        "Name": "translated_text",
                        "Type": "STRING"
                    },
                    {
                        "Name": "created_at",
                        "Type": "DATETIME"
                    },
                    {
                        "Name": "image_url",
                        "Type": "STRING"
                    },
                    {
                        "Name": "label_name",
                        "Type": "STRING"
                    },
                    {
                        "Name": "confidence",
                        "Type": "DECIMAL"
                    },
                    {
                        "Name": "source",
                        "Type": "STRING"
                    },
                    {
                        "Name": "name",
                        "Type": "STRING"
                    }
                ]
            }
        }
    },
    "LogicalTableMap": {
        "274f2c2f-89bc-4da4-8ab7-860e969cde83": {
            "Alias": "image-moderation-label",
            "DataTransforms": [
                {
                    "CreateColumnsOperation": {
                        "Columns": [
                            {
                                "ColumnName": "url_action",
                                "ColumnId": "8cf03147-11a6-4e6d-89c7-bcdcab39b2b9",
                                "Expression": "ifelse({platform} = 'twitter', concat('twitter.com/anyuser/status/',{id_str}), substring({source}, strlen('https://')+1, strlen({source})+1))"
                            }
                        ]
                    }
                },
                {
                    "ProjectOperation": {
                        "ProjectedColumns": [
                            "id_str",
                            "platform",
                            "text",
                            "translated_text",
                            "created_at",
                            "image_url",
                            "label_name",
                            "confidence",
                            "source",
                            "name",
                            "url_action"
                        ]
                    }
                }
            ],
            "Source": {
                "PhysicalTableId": "274f2c2f-89bc-4da4-8ab7-860e969cde83"
            }
        }
    }
}