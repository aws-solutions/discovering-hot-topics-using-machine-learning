{
    "PhysicalTableMap": {
        "ca723c8a-4345-40ad-897f-68e1c5d056d8": {
            "CustomSql": {
                "DataSourceArn": "{self.data_source.arn}",
                "Name": "tweet-geo-sentiment",
                "SqlQuery": "with feed as (\n    select \n        distinct\n        tw.created_at as created_at,\n        tw.id_str,\n        tw.platform,\n        tw.account_name,\n        tw.user.id_str as source,\n        tw.user.name as name,\n        tw.in_reply_to_screen_name,\n        tw.coordinates.coordinates[1] AS lon,\n        tw.coordinates.coordinates[2] AS lat,\n        tw.place.name AS place_name,\n        tw.place.place_type,\n        tw.place.country,\n        tw.retweet_count,\n        tw.quote_count,\n        tw.favorite_count,\n        tw.reply_count        \n    from socialmediadb.twfeedstorage tw\n    where tw.created_at > current_timestamp - interval '45' day    \n), sentiment as (\n    select distinct\n        sentiment,\n        created_at,\n        id_str,\n        text,\n        translated_text,\n        platform,\n        account_name\n    from socialmediadb.sentiment\n    where created_at > current_timestamp - interval '45' day\n), entity as (\n    select distinct\n        id_str,\n        entity_text,\n        platform,\n        account_name\n    from socialmediadb.entity\n    where created_at > current_timestamp - interval '45' day\n), phrase as (\n    select distinct\n        id_str,\n        phrase,\n        platform,\n        account_name\n    from socialmediadb.keyphrase\n    where created_at > current_timestamp - interval '45' day\n), tm as (\n    select distinct\n        id_str,\n        job_id,\n        job_timestamp,\n        platform,\n        topic\n    from socialmediadb.topicmappings\n    where created_at > current_timestamp - interval '45' day\n), term as (\n    select distinct\n        job_id,\n        topic,\n        term\n    from socialmediadb.topics\n    where created_at > current_timestamp - interval '45' day\n) select distinct\n    feed.id_str,\n    feed.created_at,\n    feed.platform,\n    feed.account_name,\n    feed.source,\n    feed.name,\n    feed.in_reply_to_screen_name,\n    feed.lon,\n    feed.lat,\n    feed.place_name,\n    feed.place_type,\n    feed.country,\n    feed.retweet_count,\n    feed.quote_count,\n    feed.favorite_count,\n    feed.reply_count,\n    s.sentiment,\n    s.text,\n    s.translated_text,\n    e.entity_text,\n    p.phrase,\n    tm.job_id,\n    tm.job_timestamp,\n    tm.topic,\n    term.term\nfrom feed\njoin sentiment s\n    on (feed.id_str = s.id_str)\nleft join entity e\n    on (feed.id_str = e.id_str)\nleft join phrase p\n    on (feed.id_str = p.id_str)\nleft join tm\n    on (feed.id_str = tm.id_str)\njoin term\n    on (tm.job_id = term.job_id)",
                "Columns": [
